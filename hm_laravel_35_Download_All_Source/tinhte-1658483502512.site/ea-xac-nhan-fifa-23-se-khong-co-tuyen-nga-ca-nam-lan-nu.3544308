ng đầu tiên của triển lãm First Look Of Dhaka 2019, anh tập trung theo đuổi những dự án riêng mang tính cá nhân hơn. Hiện tại anh đang cộng tác để quảng bá hình ảnh cùng thương hiệu Fuji Vietnam và giảng dạy tại P.House Education.","signature":"","signature_html":"","signature_plain_text":"","post_like_count":5,"post_attachment_count":4,"like_users":[{"user_id":907777,"username":"CBDancer","reaction_id":1},{"user_id":835842,"username":"mt9011","reaction_id":1},{"user_id":2311248,"username":"chithanhiv","reaction_id":1},{"user_id":2913339,"username":"Lâm 79hz","reaction_id":1},{"user_id":1925880,"username":"NghiepTranVINA","reaction_id":1}],"user_is_ignored":false,"post_is_published":true,"post_is_deleted":false,"post_update_date":1657893722,"post_is_first_post":true,"post_is_liked":false,"attachments":[{"attachment_id":5584465,"attachment_download_count":0,"filename":"cover_Ws_Street_life_photography.jpg","links":{"permalink":"https://photo2.tinhte.vn/data/attachment-files/2022/07/6055008_cover_Ws_Street_life_photography.jpg","data":"https://tinhte.vn/appforo/index.php?attachments/5584465/data\u0026oauth_token=0%2C1658487102%2C4ed3e58934b12fc115d56462a475f003%2Clxi7g2zolu","thumbnail":"https://imgproxy.k7.tinhte.vn/aB-oRtt-I-UZmZjM6MXW35wRDeKyOPvx4j6OFC2Atyc/w:400/plain/https://photo2.tinhte.vn/data/attachment-files/2022/07/6055008_cover_Ws_Street_life_photography.jpg","post":"https://tinhte.vn/appforo/index.php?posts/62852441/\u0026oauth_token=0%2C1658487102%2C4ed3e58934b12fc115d56462a475f003%2Clxi7g2zolu"},"attachment_width":3508,"attachment_height":2158,"attachment_is_video":false,"post_id":62852441,"permissions":{"view":true,"delete":false},"attachment_is_inserted":false},{"attachment_id":5584466,"attachment_download_count":0,"filename":"Ws_Street_life_photography_2.jpg","links":{"permalink":"https://photo2.tinhte.vn/data/attachment-files/2022/07/6055009_Ws_Street_life_photography_2.jpg","data":"https://tinhte.vn/appforo/index.php?attachments/5584466/data\u0026o