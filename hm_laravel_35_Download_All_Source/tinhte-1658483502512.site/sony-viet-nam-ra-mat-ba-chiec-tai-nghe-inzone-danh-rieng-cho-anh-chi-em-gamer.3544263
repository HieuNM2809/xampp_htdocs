orum":"https://tinhte.vn/appforo/index.php?forums/77/\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu","posts":"https://tinhte.vn/appforo/index.php?posts/\u0026thread_id=3544263\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu","first_poster":"https://tinhte.vn/appforo/index.php?users/2452461/\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu","first_poster_avatar":"https://photo2.tinhte.vn/data/avatars/m/2452/2452461.jpg?1653405194","first_post":"https://tinhte.vn/appforo/index.php?posts/62881344/\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu","last_poster":"https://tinhte.vn/appforo/index.php?users/2486261/\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu","last_post":"https://tinhte.vn/appforo/index.php?posts/62882627/\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu","image":"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062364_YUU09483.jpg"},"permissions":{"view":true,"delete":false,"follow":false,"post":false,"upload_attachment":false,"edit_image":false,"tinhtemods_redirect":false,"reply_bans":false},"rating_total":null,"rating_count":null,"creator_has_verified_badge":true,"thread_image":{"link":"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062364_YUU09483.jpg","width":1600,"height":1067,"display_mode":"cover"},"thread_thumbnail":{"link":"https://imgproxy.k7.tinhte.vn/jndqNzDHHQcJ8UL_rK6d9mM9uJcuQr3_3C1n283r3X4/rs:fill:400:400:0/plain/https://photo2.tinhte.vn/data/attachment-files/2022/07/6062364_YUU09483.jpg","width":400,"height":400},"thread_is_bookmark":false,"is_draft":false,"review_extra_data":[],"user_is_followed":false,"user_thread_count":4210,"user_follower_count":391,"user_like_count":59632,"fp_permissions":{"stick":false,"unstick":false,"delete":false},"thread_tags_extended":[{"tag_id":647,"tag":"tai nghe","tag_url":"tai-nghe"},{"tag_id":582817,"tag":"inzone","tag_url":"inzone"},{"tag_id":42,"tag":"sony","tag_url":"sony"},{"tag_id":247930,"tag":"tai nghe gaming","tag_url":"tai-nghe-gaming"},{"tag_id":2417,"tag":"tai nghe không dây","tag_url":"tai-nghe-khong-day"},{"tag_id":4348,"tag":"chơi game","tag_url":"choi-game"},{"tag_id":1285,"tag":"console","tag_url":"console"},{"tag_id":348217,"tag":"ps5","tag_url":"ps5"},{"tag_id":28649,"tag":"game pc","tag_url":"game-pc"}],"posts":[{"post_id":62881344,"thread_id":3544263,"poster_user_id":2452461,"poster_username":"P.W","post_create_date":1658458442,"post_body":"Tối 21/7, Sony Việt Nam đã tổ chức sự kiện ra mắt ba chiếc tai nghe gaming Inzone H3, H7 và H9. Có thể coi việc ra mắt những món thiết bị ngoại vi hướng riêng tới cộng đồng gamer của Sony là bước đi khá tương đồng với những gì chúng ta đã thấy trong thời gian qua với những tựa game PlayStation 4 và 5. Sony đã nhận ra tiềm năng quá lớn của thị trường thiết bị gaming PC, cũng như chính bản thân cộng đồng người chơi game PC, dẫn tới việc họ không chỉ port những game độc quyền của SIE sang nền tảng với hàng tỷ thiết bị trên toàn thế giới, mà còn muốn đánh cả vào mảng phần cứng nữa.\n \n[ATTACH=full]5591830[/ATTACH]\n \nCũng rất dễ nhận ra một thực tế khác, đó là với ba chiếc tai Inzone, Sony muốn tự đặt mình vào thị trường tai nghe gaming cao cấp, điều đó được thể hiện khá rõ ngay ở mức giá:\n \n[LIST]\n[*]Sony Inzone H3: 2.290.000 VNĐ\n[*]Sony Inzone H7: 5.300.000 VNĐ\n[*]Sony Inzone H9: 6.990.000 VNĐ\n[/LIST]\n \nKhông giống như những cặp tai nghe chơi game định hướng tới cộng đồng chơi trên console, Inzone H3, H7 và H9 có thiết kế nhắm đúng tới đối tượng gaming PC, với microphone boom kéo dài ở earcup trái, có thể tắt đi chỉ bằng một động tác đơn giản là gạt microphone lên, thay vì kéo xuống gần miệng. Còn trong khi đó, tổng thể thiết kế với tông màu đen trắng lại khiến nhiều người liên tưởng ba chiếc tai nghe Inzone được tạo ra phục vụ riêng cho cộng đồng những người sở hữu máy chơi game PS5.\n \n[ATTACH=full]5591865[/ATTACH]\n \nChỉ có một phiên bản Inzone có dây, đấy là chiếc H3, kết nối với máy tính thông qua soundcard USB đi kèm, để từ đó mô phỏng hiệu ứng âm thanh vòm khi chơi game thông qua công nghệ 360 Spatial Sound do chính Sony phát triển. Còn lại hai phiên bản H7 và H9 đều là những cặp tai nghe gaming không dây. Sony quảng cáo H7 có thời lượng pin 40 tiếng, còn H9 là 32 tiếng đồng hồ. Lý do là, H9 sở hữu công nghệ khử tiếng ồn chủ động, thứ giống hệt như trên những mẫu tai không dây cao cấp của Sony, WF và WH 1000XM.\n \n[ATTACH=full]5591834[/ATTACH]\n[ATTACH=full]5591835[/ATTACH]\n[ATTACH=full]5591836[/ATTACH]\n \nCũng vì thế, thiết kế của H7 và H9 có vài khác biệt nho nhỏ như lỗ mic ambient trên hai earcup, cùng nút bấm kích hoạt hoặc tắt chế độ noise cancelling chỉ có trên chiếc H9. Dĩ nhiên chế độ ambient sound để nghe âm thanh xung quanh cũng hiện diện, nhưng cùng lúc Sony lại đề cao khả năng chống ồn chủ động, khử tiếng gió quạt, tiếng hú của PC hay tiếng điều hòa nhiệt độ lúc anh em chơi điện tử.\n \n[ATTACH=full]5591856[/ATTACH]\n[ATTACH=full]5591855[/ATTACH]\n[ATTACH=full]5591854[/ATTACH]\n[ATTACH=full]5591857[/ATTACH]\n[ATTACH=full]5591858[/ATTACH]\n \nKhá chắc, “ngôi sao” của bộ ba sản phẩm tai nghe gaming này là chiếc Inzone H9, khi Sony không ngần ngại ứng dụng những chi tiết đã tạo ra tên tuổi của những chiếc 1000XM, ví dụ như tính năng ANC nói trên, hay chính bản thân earcup bọc da PU siêu mềm, thứ chỉ hiện diện trên H9, còn trên H3 và H7 thì sử dụng earcup bọc vải cho thoáng mát.\n \n[ATTACH=full]5591877[/ATTACH]\n \nCả hai chiếc H7 và H9 đều có dongle đi kèm. Chiếc dongle này có một công tắc chuyển đổi giữa kết nối với PC và với PS5. Nhưng cùng lúc, anh em cũng có thể kết nối tai nghe thông qua Bluetooth 5.0 thay vì phải phụ thuộc hoàn toàn vào công nghệ 2.4G. Cái này khá giống những tai nghe gaming không dây cao cấp nhất trên thị trường hiện tại, đều hỗ trợ hai chuẩn kết nối không dây. Nhờ điều này, anh em sẽ không bị bó buộc trên hai nền tảng PC và PS5, mà còn có thể dùng Inzone cho gần như tất cả mọi thiết bị khác, từ Nintendo Switch cho tới Xbox, từ iPad cho tới điện thoại di động.\n \nCòn trong khi đó, dongle của H7 và H9 khiến mình liên tưởng ngay tới những chiếc Platinum hay Gold Wireless Headset của thời kỳ PS4, trông khá to.\n \n[ATTACH=full]5591863[/ATTACH]\n[ATTACH=full]5591861[/ATTACH]\n \nKhá chắc khi các bên bắt đầu review chiếc tai nghe Inzone H9, hay H7, họ sẽ chọn những dòng title rất kêu, ví dụ như “XM5 dành cho gamer” chẳng hạn. Cũng không tránh được, vì từ cái thời điểm thông tin bị rò rỉ cho tới khi Sony chính thức bán ra thị trường 3 chiếc tai nghe Inzone, khoảng thời gian là rất ngắn. Khi cộng đồng gamer còn chưa kịp làm quen với Razer Barracuda Pro Wireless, đối thủ trực tiếp của H9, thì sản phẩm mới đã kịp ra mắt thị trường để chúng ta có những so sánh cụ thể nhất về hai cái tên phải nói là đáng gờm của thị trường tai nghe gaming cao cấp.\n \nRazer có kinh nghiệm làm đồ chơi cho game thủ, còn Sony thì có kinh nghiệm phát triển các thiết bị âm thanh cá nhân từ trước cả khi Razer tồn tại. Ấy là chưa kể, họ đã làm tai nghe có ANC từ rất lâu rồi. Câu hỏi được đặt ra cũng đơn giản. Liệu sự kết hợp của 360 Spatial Sound, của khử tiếng ồn chủ động (xét riêng tới phiên bản H9) với chất lượng âm thanh khi anh em chơi game có khiến ba chiếc tai nghe Inzone từ Sony trở thành cái tên xứng đáng trong tầm giá hay không. Câu hỏi đó chỉ vài ngày nữa mình sẽ có cho anh em.","post_body_html":"\u003c!--\u003clink rel=\"stylesheet\" href=\"https://tinhte.vn/css.php?css=bdimage\u0026amp;style=85\u0026amp;dir=LTR\u0026amp;d=1657705608\" /\u003e--\u003e\u003cspan class=\"xf-body-paragraph\"\u003eTối 21/7, \u003ca href=\"https://tinhte.vn/tag/sony\" class=\"Tinhte_XenTag_TagLink\"\u003eSony\u003c/a\u003e Việt Nam đã tổ chức sự kiện ra mắt ba chiếc \u003ca href=\"https://tinhte.vn/tag/tai-nghe-gaming\" class=\"Tinhte_XenTag_TagLink\"\u003etai nghe gaming\u003c/a\u003e \u003ca href=\"https://tinhte.vn/tag/inzone\" class=\"Tinhte_XenTag_TagLink\"\u003eInzone\u003c/a\u003e H3, H7 và H9. Có thể coi việc ra mắt những món thiết bị ngoại vi hướng riêng tới cộng đồng gamer của Sony là bước đi khá tương đồng với những gì chúng ta đã thấy trong thời gian qua với những tựa game PlayStation 4 và 5. Sony đã nhận ra tiềm năng quá lớn của thị trường thiết bị gaming PC, cũng như chính bản thân cộng đồng người \u003ca href=\"https://tinhte.vn/tag/choi-game\" class=\"Tinhte_XenTag_TagLink\"\u003echơi game\u003c/a\u003e PC, dẫn tới việc họ không chỉ port những game độc quyền của SIE sang nền tảng với hàng tỷ thiết bị trên toàn thế giới, mà còn muốn đánh cả vào mảng phần cứng nữa.\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591830/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09523.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062373_YUU09523.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cspan class=\"xf-body-paragraph\"\u003e\u003cbr /\u003e\n \u003cbr /\u003e\nCũng rất dễ nhận ra một thực tế khác, đó là với ba chiếc tai Inzone, Sony muốn tự đặt mình vào thị trường \u003ca href=\"https://tinhte.vn/tag/tai-nghe\" class=\"Tinhte_XenTag_TagLink\"\u003etai nghe\u003c/a\u003e gaming cao cấp, điều đó được thể hiện khá rõ ngay ở mức giá:\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\u003cul\u003e\n\u003cli\u003eSony Inzone H3: 2.290.000 VNĐ\u003c/li\u003e\n\u003cli\u003eSony Inzone H7: 5.300.000 VNĐ\u003c/li\u003e\n\u003cli\u003eSony Inzone H9: 6.990.000 VNĐ\u003c/li\u003e\n\u003c/ul\u003e\u003cspan class=\"xf-body-paragraph\"\u003e \u003cbr /\u003e\nKhông giống như những cặp tai nghe chơi game định hướng tới cộng đồng chơi trên \u003ca href=\"https://tinhte.vn/tag/console\" class=\"Tinhte_XenTag_TagLink\"\u003econsole\u003c/a\u003e, Inzone H3, H7 và H9 có thiết kế nhắm đúng tới đối tượng gaming PC, với microphone boom kéo dài ở earcup trái, có thể tắt đi chỉ bằng một động tác đơn giản là gạt microphone lên, thay vì kéo xuống gần miệng. Còn trong khi đó, tổng thể thiết kế với tông màu đen trắng lại khiến nhiều người liên tưởng ba chiếc tai nghe Inzone được tạo ra phục vụ riêng cho cộng đồng những người sở hữu máy chơi game \u003ca href=\"https://tinhte.vn/tag/ps5\" class=\"Tinhte_XenTag_TagLink\"\u003ePS5\u003c/a\u003e.\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591865/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09476.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062408_YUU09476.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cspan class=\"xf-body-paragraph\"\u003e\u003cbr /\u003e\n \u003cbr /\u003e\nChỉ có một phiên bản Inzone có dây, đấy là chiếc H3, kết nối với máy tính thông qua soundcard USB đi kèm, để từ đó mô phỏng hiệu ứng âm thanh vòm khi chơi game thông qua công nghệ 360 Spatial Sound do chính Sony phát triển. Còn lại hai phiên bản H7 và H9 đều là những cặp tai nghe gaming không dây. Sony quảng cáo H7 có thời lượng pin 40 tiếng, còn H9 là 32 tiếng đồng hồ. Lý do là, H9 sở hữu công nghệ khử tiếng ồn chủ động, thứ giống hệt như trên những mẫu tai không dây cao cấp của Sony, WF và WH 1000XM.\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591834/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09499.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062377_YUU09499.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591835/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09498.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062378_YUU09498.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591836/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09503.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062379_YUU09503.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cspan class=\"xf-body-paragraph\"\u003e\u003cbr /\u003e\n \u003cbr /\u003e\nCũng vì thế, thiết kế của H7 và H9 có vài khác biệt nho nhỏ như lỗ mic ambient trên hai earcup, cùng nút bấm kích hoạt hoặc tắt chế độ noise cancelling chỉ có trên chiếc H9. Dĩ nhiên chế độ ambient sound để nghe âm thanh xung quanh cũng hiện diện, nhưng cùng lúc Sony lại đề cao khả năng chống ồn chủ động, khử tiếng gió quạt, tiếng hú của PC hay tiếng điều hòa nhiệt độ lúc anh em chơi điện tử.\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591856/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09488.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062399_YUU09488.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591855/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09490.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062398_YUU09490.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591854/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09492.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062397_YUU09492.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591857/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09494.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062400_YUU09494.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591858/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09514.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062401_YUU09514.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cspan class=\"xf-body-paragraph\"\u003e\u003cbr /\u003e\n \u003cbr /\u003e\nKhá chắc, “ngôi sao” của bộ ba sản phẩm tai nghe gaming này là chiếc Inzone H9, khi Sony không ngần ngại ứng dụng những chi tiết đã tạo ra tên tuổi của những chiếc 1000XM, ví dụ như tính năng ANC nói trên, hay chính bản thân earcup bọc da PU siêu mềm, thứ chỉ hiện diện trên H9, còn trên H3 và H7 thì sử dụng earcup bọc vải cho thoáng mát.\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591877/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09517.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062420_YUU09517.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cspan class=\"xf-body-paragraph\"\u003e\u003cbr /\u003e\n \u003cbr /\u003e\nCả hai chiếc H7 và H9 đều có dongle đi kèm. Chiếc dongle này có một công tắc chuyển đổi giữa kết nối với PC và với PS5. Nhưng cùng lúc, anh em cũng có thể kết nối tai nghe thông qua Bluetooth 5.0 thay vì phải phụ thuộc hoàn toàn vào công nghệ 2.4G. Cái này khá giống những tai nghe gaming không dây cao cấp nhất trên thị trường hiện tại, đều hỗ trợ hai chuẩn kết nối không dây. Nhờ điều này, anh em sẽ không bị bó buộc trên hai nền tảng PC và PS5, mà còn có thể dùng Inzone cho gần như tất cả mọi thiết bị khác, từ Nintendo Switch cho tới Xbox, từ iPad cho tới điện thoại di động.\u003cbr /\u003e\n \u003cbr /\u003e\nCòn trong khi đó, dongle của H7 và H9 khiến mình liên tưởng ngay tới những chiếc Platinum hay Gold Wireless Headset của thời kỳ PS4, trông khá to.\u003cbr /\u003e\n \u003cbr /\u003e\n\u003c/span\u003e\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591863/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09575.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062406_YUU09575.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cbr /\u003e\n\n\t\n\n\u003cspan class=\"bdImage_attachImage\" style=\"width:1600px\"\u003e\n\t\u003cspan class=\"inner\" style=\"padding-bottom:66.687500%\"\u003e\n\t\t\n\n\t\t\u003cimg src=\"https://tinhte.vn/appforo/index.php?attachments/5591861/data\u0026oauth_token=0%2C1658487111%2C3429d3191bf6a6d9956126aded84c3e7%2Clxi7g2zolu\"\n\t\t\t alt=\"YUU09576.JPG\"\n\t\t\t data-height=\"1067\"\n\t\t\t data-permalink=\"https://photo2.tinhte.vn/data/attachment-files/2022/07/6062404_YUU09576.jpg\"\n\t\t\t data-width=\"1600\" /\u003e\n\n\n\t\u003c/span\u003e\n\u003c/span\u003e\n\n\n\t\n\u003cspan class=\"xf-body-paragraph\"\u003e\u003cbr /\u003e\n \u003cbr /\u003e\nKhá chắc khi các bên bắt đầu review chiếc tai nghe Inzone H9, hay H7, họ sẽ chọn những dòng title rất kêu, ví dụ như “XM5 dành cho gamer” chẳng hạn. Cũng không tránh được, vì từ cái thời điểm thông tin bị rò rỉ cho tới khi Sony chính thức bán ra thị trường 3 chiếc tai nghe Inzone, khoảng thời gian là rất ngắn. Khi cộng đồng gamer còn chưa kịp làm quen với Razer Barracuda Pro Wireless, đối thủ trực tiếp của H9, thì sản phẩm mới đã kịp ra mắt thị trường để chúng ta có những so sánh cụ thể nhất về hai cái tên phải nói là đáng gờm của thị trường tai nghe gaming cao cấp.\u003cbr /\u003e\n \u003cbr /\u003e\nRazer có kinh nghiệm làm đồ chơi cho game thủ, còn Sony thì có kinh nghiệm phát triển các thiết bị âm thanh cá nhân từ trước cả khi Razer tồn tại. Ấy là chưa kể, họ đã làm tai nghe có ANC từ rất lâu rồi. Câu hỏi được đặt ra cũng đơn giản. Liệu sự kết hợp của 360 Spatial Sound, của khử tiếng ồn chủ động (xét riêng tới phiên bản H9) với chất lượng âm thanh khi anh em chơi game có khiến ba chiếc tai nghe Inzone từ Sony trở thành cái tên xứng đáng trong tầm giá hay không. Câu hỏi đó chỉ vài ngày nữa mình sẽ có cho anh em.\u003c/span\u003e","post_body_plain_text":"Tối 21/7, Sony Việt Nam đã tổ chức sự kiện ra mắt ba chiếc tai nghe gaming Inzone H3, H7 và H9. Có thể coi việc ra mắt những món thiết bị ngoại vi hướng riêng tới cộng đồng gamer của Sony là bước đi khá tương đồng với những gì chúng ta đã thấy trong thời gian qua với những tựa game PlayStation 4 và 5. Sony đã nhận ra tiềm năng quá lớn của thị trường thiết bị gaming PC, cũng như chính bản thân cộng đồng người chơi game PC, dẫn tới việc họ không chỉ port những game độc quyền của SIE sang nền tảng với hàng tỷ thiết bị trên toàn thế giới, mà còn muốn đánh cả vào mảng phần cứng nữa.\n \n\n \nCũng rất dễ nhận ra một thực tế khác, đó là với ba chiếc tai Inzone, Sony muốn tự đặt mình vào thị trường tai nghe gaming cao cấp, điều đó được thể hiện khá rõ ngay ở mức giá:\n \n\nSony Inzone H3: 2.290.000 VNĐ\nSony Inzone H7: 5.300.000 VNĐ\nSony Inzone H9: 6.990.000 VNĐ\n\n \nKhông giống như những cặp tai nghe chơi game định hướng tới cộng đồng chơi trên console, Inzone H3, H7 và H9 có thiết kế nhắm đúng tới đối tượng gaming PC, với microphone boom kéo dài ở earcup trái, có thể tắt đi chỉ bằng một động tác đơn giản là gạt microphone lên, thay vì kéo xuống gần miệng. Còn trong khi đó, tổng thể thiết kế với tông màu đen trắng lại khiến nhiều người liên tưởng ba chiếc tai nghe Inzone được tạo ra phục vụ riêng cho cộng đồng những người sở hữu máy chơi game PS5.\n \n\n \nChỉ có một phiên bản Inzone có dây, đấy là chiếc H3, kết nối với máy tính thông qua soundcard USB đi kèm, để từ đó mô phỏng hiệu ứng âm thanh vòm khi chơi game thông qua công nghệ 360 Spatial Sound do chính Sony phát triển. Còn lại hai phiên bản H7 và H9 đều là những cặp tai nghe gaming không dây. Sony quảng cáo H7 có thời lượng pin 40 tiếng, còn H9 là 32 tiếng đồng hồ. Lý do là, H9 sở hữu công nghệ khử tiếng ồn chủ động, thứ giống hệt như trên những mẫu tai không dây cao cấp của Sony, WF và WH 1000XM.\n \n\n\n\n \nCũng vì thế, thiết kế của H7 và H9 có vài khác biệt nho nhỏ như lỗ mic ambient trên hai earcup, cùng nút bấm kích hoạt hoặc tắt chế độ noise cancelling chỉ có trên chiếc H9. Dĩ nhiên chế độ ambient sound để nghe âm thanh xung quanh cũng hiện diện, nhưng cùng lúc Sony lại đề cao khả năng chống ồn chủ động, khử tiếng gió quạt, tiếng hú của PC hay tiếng điều hòa nhiệt độ lúc anh em chơi điện tử.\n \n\n\n\n\n\n \nKhá chắc, “ngôi sao” của bộ ba sản phẩm tai nghe gaming này là chiếc Inzone H9, khi Sony không ngần ngại ứng dụng những chi tiết đã tạo ra tên tuổi của những chiếc 1000XM, ví dụ như tính năng ANC nói trên, hay chính bản thân earcup bọc da PU siêu mềm, thứ chỉ hiện diện trên H9, còn trên H3 và H7 thì sử dụng earcup bọc vải cho thoáng mát.\n \n\n \nCả hai chiếc H7 và H9 đều có dongle đi kèm. Chiếc dongle này có một công tắc chuyển đổi giữa kết nối với PC và với PS5. Nhưng cùng lúc, anh em cũng có thể kết nối tai nghe thông qua Bluetooth 5.0 thay vì phải phụ thuộc hoàn toàn vào công nghệ 2.4G. Cái này khá giống những tai nghe gaming không dây cao cấp nhất trên thị trường hiện tại, đều hỗ trợ hai chuẩn kết nối không dây. Nhờ điều này, anh em sẽ không bị bó buộc trên hai nền tảng PC và PS5, mà còn có thể dùng Inzone cho gần như tất cả mọi thiết bị khác, từ Nintendo Switch cho tới Xbox, từ iPad cho tới điện thoại di động.\n \nCòn trong khi đó, dongle của H7 và H9 khiến mình liên tưởng ngay tới những chiếc Platinum hay Gold Wireless Headset của thời kỳ PS4, trông khá to.\n \n\n\n \nKhá chắc khi các bên bắt đầu review chiếc tai nghe Inzone H9, hay H7, họ sẽ chọn những dòng title rất kêu, ví dụ như “XM5 dành cho gamer” chẳng hạn. Cũng không tránh được, vì từ cái thời điểm thông tin bị rò rỉ cho tới khi Sony chính thức bán ra thị trường 3 chiếc tai nghe Inzone, khoảng thời gian là rất ngắn. Khi cộng đồng gamer còn chưa kịp làm quen với Razer Barracuda Pro Wireless, đối thủ trực tiếp của H9, thì sản phẩm mới đã kịp ra mắt thị trường để chúng ta có những so sánh cụ thể nhất về hai cái tên phải nói là đáng gờm của thị trường tai nghe gaming cao cấp.\n \nRazer có kinh nghiệm làm đồ chơi cho game thủ, còn Sony thì có kinh nghiệm phát triển các thiết bị âm thanh cá nhân từ trước cả khi Razer tồn tại. Ấy là chưa kể, họ đã làm tai nghe có ANC từ rất lâu rồi. Câu hỏi được đặt ra cũng đơn giản. Liệu sự kết hợp của 360 Spatial Sound, của khử tiếng ồn chủ động (xét riêng tới phiên bản H9) với chất lượng âm thanh khi anh em chơi game có khiến ba chiếc tai nghe Inzone từ Sony trở thành cái tên xứng đáng trong tầm giá hay không. Câu hỏi đó chỉ vài ngày nữa mình sẽ có 