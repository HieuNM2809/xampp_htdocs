ast_poster":"https://tinhte.vn/appforo/index.php?users/2047029/\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu","last_post":"https://tinhte.vn/appforo/index.php?posts/62870096/\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu"},"permissions":{"view":true,"delete":false,"follow":false,"post":false,"upload_attachment":false,"edit_prefix":false,"edit_tags":false,"edit_title":false,"tinhtemods_redirect":false,"reply_bans":false},"rating_total":null,"rating_count":null,"creator_has_verified_badge":false,"thread_is_bookmark":false,"is_draft":false,"user_is_followed":false,"user_thread_count":430,"user_follower_count":7,"user_like_count":1518,"fp_permissions":{"stick":false,"unstick":false,"delete":false},"thread_tags_extended":[{"tag_id":463725,"tag":"covid-19","tag_url":"covid-19"},{"tag_id":577946,"tag":"nhà thuốc ngọc anh","tag_url":"nha-thuoc-ngoc-anh"},{"tag_id":465554,"tag":"vaccine covid-19","tag_url":"vaccine-covid-19"}],"forum":{"forum_id":700,"forum_title":"Sức khỏe","forum_description":"","forum_thread_count":14572,"forum_post_count":363215,"forum_prefixes":[{"group_title":"Phân loại","group_prefixes":[{"prefix_id":120,"prefix_title":"Trên tay"},{"prefix_id":121,"prefix_title":"Trải nghiệm"},{"prefix_id":122,"prefix_title":"So sánh"},{"prefix_id":123,"prefix_title":"Review"},{"prefix_id":124,"prefix_title":"Breaking news"},{"prefix_id":125,"prefix_title":"Tổng hợp thông tin"},{"prefix_id":126,"prefix_title":"Phân tích"},{"prefix_id":127,"prefix_title":"Thủ thuật"},{"prefix_id":128,"prefix_title":"Tin tức"},{"prefix_id":129,"prefix_title":"Tâm sự"},{"prefix_id":115,"prefix_title":"Hỏi đáp"}]}],"thread_default_prefix_id":0,"thread_prefix_is_required":false,"links":{"permalink":"https://tinhte.vn/forums/suc-khoe.700/","detail":"https://tinhte.vn/appforo/index.php?forums/700/\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu","sub-categories":"https://tinhte.vn/appforo/index.php?categories/\u0026parent_forum_id=700\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu","sub-forums":"https://tinhte.vn/appforo/index.php?forums/\u0026parent_forum_id=700\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu","threads":"https://tinhte.vn/appforo/index.php?threads/\u0026forum_id=700\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu","followers":"https://tinhte.vn/appforo/index.php?forums/700/followers\u0026oauth_token=0%2C1658487098%2C6f9e0c782cc13ecf47929fa0cbea628b%2Clxi7g2zolu"},"permissions":{"view":true,"edit":false,"delete":false,"create_thread":false,"upload_attachment":false,"tag_thread":false,"follow":false},"forum_is_followed":false,"forum_thumbnail_url":""}},{"content_type":"thread","content_id":3541260,"thread_id":3541260,"forum_id":700,"thread_title":"Biến thể phụ BA.2.75 được người dùng Twitter đặt tên Centaurus, WHO phủ nhận cách đặt tên này","thread_view_count":2390,"creator_user_id":17,"creator_username":"Hassler","thread_create_date":1657802126,"thread_update_date":1658224074,"thread_is_new":false,"user_is_ignored":false,"thread_post_count":38,"thread_is_published":true,"thread_is_deleted":false,"thread_is_sticky":false,"thread_is_followed":false,"first_post":{"post_id":62850206,"thread_id":3541260,"poster_user_id":17,"poster_username":"Hassler","post_create_date":1657801771,"post_body":"Câu chuyện các biến thể mới của SARS-CoV-2 giờ đây là câu chuyện không còn quá lạ đối với nhiều người bởi sự đột biến của con virus gây đại dịch này vẫn đang tiếp tục diễn ra. Gần đây tiếp tục xuất hiện biến thể phụ BA.2.75 của Omicron, được cho là có khả năng lây lan còn cao hơn nữa và đang chiếm ưu thế về số ca mắc mới ở nhiều quốc gia.\n \nVào thời điểm hiện tại WHO khẳng định vẫn chưa xếp biến thể phụ này vào nhóm đáng lo ngại, tuy nhiên nhiều chuyên gia cho rằng nó có thể vượt qua hàng rào bảo vệ được dựng nên bởi những lần nhiễm trước hay bởi vaccine. Điều này sẽ khiến hệ miễn dịch trở nên suy yếu hơn và dù có thể mắc BA.2.75 không trực tiếp gây nguy hiểm hay tử vong cho bệnh nhân nhưng rất có thể những triệu chứng hậu covid sẽ nặng nề hơn những biến thể hiện tại.\n \nTheo lời của 1 chuyên gia virus học ở Mayo Clinic thì hiện vẫn chưa có dấu hiệu rõ ràng cho thấy nó gây tăng mạnh các ca mắc mới. Còn theo lời của người đứng đầu nhóm giải mã trình tự gen của CDC Mỹ thì họ đã phát hiện biến thể phụ này ở nhiều nơi, kể cả những nơi không được giám sát nhiều, vậy nên rõ ràng đây là 1 chỉ điểm của việc nó đang lây lan mạnh hơn bình thường.\n \n[URL]https://twitter.com/xabitron1/status/1546968464274907137?s=20\u0026t=x2j1aDfMWnMa_kAmfLuhKQ[/URL]\n \nLiên quan đến biến thể này còn là câu chuyện đặt tên biến thể. Điều tưởng đã được chốt lại khi WHO đề nghị dùng bảng chữ cái La Mã để đặt tên các biến thể mới. Nhưng nhiều người với trí tưởng tượng phong phú vẫn thích tự đặt tên hơn là theo cách này. Ở trên Twitter đã có người đặt tên biến chủng phụ BA.2.75 này với cái tên khá long lanh, đó là Centaurus, là chòm sao Nhân Mã. Cụm từ này được rất nhiều người hưởng ứng và sử dụng, kể cả trong những cuộc họp chính thức, đến cả 1 trong những cựu cố vấn cao cấp về Covid-19 của chính phủ Mỹ cũng dùng cụm từ này. WHO thì như nói ở trên chưa xếp nó vào nhóm đáng lo, vì vậy chưa đặt tên theo bảng chữ cái, và không khuyến khích việc tự đặt tên các biến thể theo cách tự phát. Bởi như họ tuyên bố trước đây, điều này có thể gây nên những hiểu lầm và hiềm khích không đáng có.\n \n[B]Tham khảo [/B][URL='https://time.com/6195495/covid-omicron-variant-mutant-contagious-ba2-75/'][B]Time[/B][/URL]","post_body_html":"\u003c!--\u003clink rel=\"stylesheet\" href=\"https://tinhte.vn/css.php?css=bdlinkexpander\u0026amp;style=85\u0026amp;dir=LTR\u0026amp;d=1657705608\" /\u003e\u003cscript src=\"https://tinhte.vn/js/bdLinkExpander/script.min.js?_v=ee7aa221\"\u003e\u003c/script\u003e--\u003e\u003cspan class=\"xf-body-paragraph\"\u003eCâu chuyện các biế